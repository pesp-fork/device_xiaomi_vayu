# Allow binder communication with hal_touchfeature_default
binder_call(hal_lineage_fod_msmnile, hal_touchfeature_default)

allow hal_lineage_fod_msmnile sysfs_graphics:dir r_dir_perms;
allow hal_lineage_fod_msmnile sysfs_graphics:file rw_file_perms;
